{{define "home"}}
{{template "base" .}}
<div class="container">
<h2 class="header">Tasks</h2>
<br/>
<br/>
{{range .tasks}}
<div class="content">

  <h4 class="header">{{ .Summary}}</h4>
<div class="taskinline">
<pre>{{.Content}}</pre>
<p>{{.Scheduled}}
</p>
</div>
</div>
<br/>
{{end}}
</div>
{{end}}


